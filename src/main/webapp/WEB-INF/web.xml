<?xml version="1.0" encoding="UTF-8"?>

<web-app id="cycleK-webApp" version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">

    <display-name>Hello World HTML</display-name>
    <description>Example of minimal HTML page writing "Hello, world!"</description>

    <welcome-file-list>
        <welcome-file>/html/hello.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>it.unipd.dei.cyclek.servlet.HelloWorldServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/tette</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/dado</url-pattern>
    </servlet-mapping>

</web-app>